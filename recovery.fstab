/boot		emmc	/dev/block/bootdevice/by-name/boot
/recovery	emmc	/dev/block/bootdevice/by-name/recovery	flags=backup=1
/system		ext4	/dev/block/bootdevice/by-name/system    fsflags=discard
/system_image	emmc	/dev/block/bootdevice/by-name/system
/data		ext4	/dev/block/bootdevice/by-name/userdata	flags=encryptable=/dev/block/bootdevice/by-name/encrypt;fsflags=discard
/cache		ext4	/dev/block/bootdevice/by-name/cache     fsflags=discard
#/persist        ext4    /dev/block/bootdevice/by-name/persist   fsflags=discard
#/sns		ext4	/dev/block/bootdevice/by-name/sns       fsflags=discord
#/persist-lg	ext4	/dev/block/bootdevice/by-name/drm	fsflags=discard
#/mpt		ext4	/dev/block/bootdevice/by-name/mpt       fsflags=discard
#/carrier	ext4	/dev/block/bootdevice/by-name/operatorlogging fsflags=discard

# set /vendor removable so that TWRP does not throw errors for users who are not using treble partition
#/vendor		ext4	/dev/block/bootdevice/by-name/vendor     flags=backup=1;fsflags=discard;wipeingui;removable

# /misc is disabled because ORS commands to factory reset on boot key combo
/misc		emmc	/dev/block/bootdevice/by-name/misc
/efs1		emmc	/dev/block/bootdevice/by-name/modemst1	flags=backup=1;display="EFS"
/efs2		emmc	/dev/block/bootdevice/by-name/modemst2	flags=backup=1;subpartitionof=/efs1
/efsg		emmc	/dev/block/bootdevice/by-name/fsg	flags=backup=1;subpartitionof=/efs1
# /firmware is disabled because it isn't needed until encryption works
/firmware	vfat	/dev/block/bootdevice/by-name/modem	flags=backup=1;display="Firmware";mounttodecrypt;fsflags=context=u:object_r:firmware_file:s0
# /modem and /persist are added for flashing OTA files
/modem		emmc	/dev/block/bootdevice/by-name/modem	flags=display="Modem";flashimg
/persist	emmc	/dev/block/bootdevice/by-name/persist	flags=display="Persist";flashimg
/external_sd	auto	/dev/block/mmcblk0p1 /dev/block/mmcblk0 flags=display="Micro SDCard";storage;wipeingui;removable;encryptable=/dev/block/bootdevice/by-name/encrypt
/usb-otg	auto	/dev/block/sdh1 /dev/block/sdh flags=display="USB-OTG";storage;wipeingui;removable
/sd-ext		ext4	/dev/block/mmcblk0p3 /dev/block/mmcblk0p2	flags=display="SD Ext";backup=1;wipeduringfactoryreset;removable
